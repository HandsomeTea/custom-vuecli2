  "dependencies": {
    "agentkeepalive": "^4.2.1",
    "axios": "^0.27.2",
    "core-js": "^3.21.1",
    "element-ui": "^2.15.9",
    "vant": "^2.12.37",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.26.7",
    "vue-property-decorator": "^9.1.2",
